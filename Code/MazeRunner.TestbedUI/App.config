<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>


  <system.diagnostics>
    <sources>
      <source name="EnginesTestbench"
              switchName="EnginesTestbenchSwitch"
              switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <clear/>
          <remove name="Default" />
        </listeners>
      </source>
      <source name="EnginesFactorySingleton"
              switchName="EnginesFactorySingletonSwitch"
              switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <clear/>
          <remove name="Default" />
        </listeners>
      </source>
      <source name="MazeRunnerSimpleDepthFirstEngine"
              switchName="MazeRunnerSimpleDepthFirstEngineSwitch"
              switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <clear/>
          <remove name="Default" />
        </listeners>
      </source>
      <source name="MazeRunnerDepthFirstAvoidPathfoldingEngine"
              switchName="MazeRunnerDepthFirstAvoidPathfoldingEngineSwitch"
              switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <clear/>
          <remove name="Default" />
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="EnginesTestbenchSwitch" value="Off" />
      <add name="EnginesFactorySingletonSwitch" value="Off" />
      <add name="MazeRunnerSimpleDepthFirstEngineSwitch" value="Off" />
      <add name="MazeRunnerDepthFirstAvoidPathfoldingEngineSwitch" value="Off" />
    </switches>
  </system.diagnostics>

